@switch (this.CSFFontAwesomeIconType)
{
    case CSFFontAwesomeIconType.CHEVRON_LEFT:
        <i class="fas @this.CssClass fa-chevron-left"></i>
        break;
    case CSFFontAwesomeIconType.CHEVRON_RIGHT:
        <i class="fas @this.CssClass fa-chevron-right"></i>
        break;
    case CSFFontAwesomeIconType.EDIT:
        <i class="fas @this.CssClass fa-pencil-alt"></i>
        break;
    case CSFFontAwesomeIconType.FILTER:
        <i class="fas @this.CssClass fa-filter"></i>
        break;
    case CSFFontAwesomeIconType.SEARCH:
        <i class="fas @this.CssClass fa-search"></i>
        break;
    case CSFFontAwesomeIconType.ORDER_DESC:
        <i class="fas @this.CssClass fa-arrow-down"></i>
        break;
    case CSFFontAwesomeIconType.VIEW:
        <i class="fas @this.CssClass fa-eye"></i>
        break;
    case CSFFontAwesomeIconType.NOTIFICATION:
        <i class="fas @this.CssClass fa-bell"></i>
        break;
    case CSFFontAwesomeIconType.CLIPBOARD:
        <i class="fas @this.CssClass fa-clipboard"></i>
        break;
    case CSFFontAwesomeIconType.BOOK:
        <i class="fas @this.CssClass fa-book"></i>
        break;
    case CSFFontAwesomeIconType.EXCLAMATION:
        <i class="fas fa-exclamation-circle"></i>
        break;
    case CSFFontAwesomeIconType.USER:
        <i class="fas @this.CssClass fa-user-circle"></i>
        break;
}