@inherits CSFFontAwesomeIconBase

@switch (this.CSFFontAwesomeIconType)
{
    case CSFFontAwesomeIconType.CHEVRON_LEFT:
        <i class="fas fa-chevron-left"></i>
        break;
    case CSFFontAwesomeIconType.CHEVRON_RIGHT:
        <i class="fas fa-chevron-right"></i>
        break;
    case CSFFontAwesomeIconType.EDIT:
        <i class="fas fa-pencil-alt"></i>
        break;
    case CSFFontAwesomeIconType.FILTER:
        <i class="fas fa-filter"></i>
        break;
    case CSFFontAwesomeIconType.SEARCH:
        <i class="fas fa-search"></i>
        break;
    case CSFFontAwesomeIconType.ORDER_DESC:
        <i class="fas fa-arrow-down"></i>
        break;
    case CSFFontAwesomeIconType.VIEW:
        <i class="fas fa-eye"></i>
        break;
    case CSFFontAwesomeIconType.NOTIFICATION:
        <i class="fas fa-bell"></i>
        break;
    case CSFFontAwesomeIconType.CLIPBOARD:
        <i class="fas fa-clipboard"></i>
        break;
    case CSFFontAwesomeIconType.BOOK:
        <i class="fas fa-book"></i>
        break;
    case CSFFontAwesomeIconType.EXCLAMATION:
        <i class="fas fa-exclamation-circle"></i>
        break;
}